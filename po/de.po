# Turn the machine completely off
#: ../lxsession-logout/lxsession-logout.c:699
msgid "Sh_utdown"
msgstr "Her_unterfahren"

# Make the machine shut down and restart itself
#: ../lxsession-logout/lxsession-logout.c:710
msgid "_Reboot"
msgstr "Neu sta_rten"

# Have the machine pause, leaving things exactly where they are
#: ../lxsession-logout/lxsession-logout.c:721
msgid "_Suspend"
msgstr "_Standby"

# Have the machine close all jobs and go into a low energy state - still logged in
#: ../lxsession-logout/lxsession-logout.c:732
msgid "_Hibernate"
msgstr "Ru_hezustand"

# Work in another user account while keeping the current one open and read to return to
#: ../lxsession-logout/lxsession-logout.c:743
msgid "S_witch User"
msgstr "Benutzer _wechseln"

#: ../lxsession-logout/lxsession-logout.c:754
